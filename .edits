# Open the last file you edited in Vim.
alias lvim="vim -c \"normal '0\""

# Edit .bash_profile
alias ebp="vim /Users/Em/.bash_profile"

# Edit .bashrc
alias ebrc="vim /Users/Em/.bashrc"

# Edit aliases
alias ealiase="vim /Users/Em/.aliases"

# Edit function library
alias eflib="vim /Users/Em/.myscripts/Library.sh"

# Edit Hammerspoon init.lua
alias eham="vim /Users/Em/.hammerspoon/init.lua"

# Edit Karabiner private.xml
alias karabin="vim '/Users/Em/Library/Application Support/Karabiner/private.xml'"


# Edit vimrc
alias evim="vim /Users/Em/.vim/vimrc"

# Edit nvim init.vim
alias envim="vim /Users/Em/.config/nvim/init.vim"
