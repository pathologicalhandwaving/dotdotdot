@book{${1:citekey},
	author={${2:author}},
	title={${3:title}},
	subtitle={${4:subtitle}},
	year={${5:YYYY}},
	volume={${6:vol}},
	edition={${7:edition}},
	address={${8:somewhere}},
	publisher={${9:publisher}},
	editor={${10:editor}},
	pages={${10:NN}--${11:NN}},
	note={${12:note}}
}
$0
endsnippet

