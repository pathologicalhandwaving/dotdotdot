priority -50

snippet article
@article{${1:citekey},
	author={${2:author}},
	title={${3:title}},
	journaltitle={${4:journal}},
	volume={${5:NN}},
	number={${6:NN}},
	year={${7:YYYY}},
	pages={${8:NN}--${9:NN}},
	note={${10:note}}
}
$0
endsnippet

