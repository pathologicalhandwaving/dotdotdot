# LaTeX and BibTeX Utility Aliases

# TeXInfo Path
export PATH="/usr/local/opt/texinfo/bin:$PATH"

# Cleanup Auxiliary Files
alias texclean="rm *.log *.bbl *.blg *.out *.gz *.synctex.gz *.synctex.gz(busy) *.tex.bak *.sty.bak"

#alias biblib="cd /Library/TeX/Local/bibtex/bib/local"

# Central Bib Path

# Edit Central Bibliography File
alias bibfile="vim /Users/Em/Repos/bibTexDB/central-bibliography.bib"

# Where is my Bib
alias 'whereismybib'="echo 'The Central Bibliography is in Repos/bibTexDB'"

