# LaTeX and BibTeX Utility Aliases


# Cleanup Auxiliary Files
alias texclean="rm *.aux *.log *.bbl *.blg *.out *.gz *.synctex.gz *.tex.bak *.sty.bak"

# Central Bib Path
export BIBPATH="/Users/Em/.pubs/bib/"

# Edit Central Bibliography File
alias bibfile="vim /Users/Em/.pubs/bib/central.bib"


# TeX Search
function texhelp {
  elinks http://www.google.com/search?q="$1"+site:http://giss.nasa.gov/tools/latex
}

